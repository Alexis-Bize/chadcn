        {
          "path": "registry/lazy-dialog/providers/lazy-dialog/shared.types.ts",
          "type": "registry:component"
        },
        {
          "path": "registry/lazy-dialog/providers/lazy-dialog/partials/with-custom-props.tsx",
          "type": "registry:component"
        },
        {
          "path": "registry/lazy-dialog/providers/lazy-dialog/partials/without-custom-props.tsx",
          "type": "registry:component"
